<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="com.yeps.model">

	<select id="listReview" resultType="ReviewDBBean">
		select * from yeps_review order by rvnum desc
	</select>
 	
	<insert id="insertReview" parameterType="ReviewDBBean">
  		insert into yeps_review values (yeps_review_seq.nextval, #{no1}, #{no2}, #{writer}, #{content}, sysdate, #{gradepoint}, #{filenum}, #{ip})
  	</insert>

</mapper>		
