<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yeps.model">
	<select id="listMenu" resultType="MenuDTO">
		select * from yeps_menu
	</select>
	
	<delete id="deleteMenuByMemberNumber" parameterType="int">
		delete yeps_menu where mnum = #{mnum}
	</delete>
</mapper>